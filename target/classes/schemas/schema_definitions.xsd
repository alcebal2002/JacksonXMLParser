<xs:documentation>A reason code</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="15"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="YNFlag">


<xs:annotation>

<xs:documentation>Y or N flag</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:pattern value="(Y|N)"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="RiskGroup">


<xs:annotation>

<xs:documentation/>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="64"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="Trader">


<xs:annotation>

<xs:documentation>Trader of the Trade.</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="64"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="SalesPerson">


<xs:annotation>

<xs:documentation>SalesPerson of the Trade.</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="64"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="Quantity">


<xs:annotation>

<xs:documentation>Number of units</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:decimal">

<xs:totalDigits value="18"/>

<xs:fractionDigits value="10"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="AverageCost">


<xs:annotation>

<xs:documentation>Average Cost</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:decimal">

<xs:totalDigits value="18"/>

<xs:fractionDigits value="10"/>

</xs:restriction>

</xs:simpleType>


