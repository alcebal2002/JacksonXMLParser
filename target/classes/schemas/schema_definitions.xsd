</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="64"/>

</xs:restriction>

</xs:simpleType>


<xs:simpleType name="PayRec">


<xs:annotation>

<xs:documentation/>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="1"/>

</xs:restriction>

</xs:simpleType>

<!-- Added for the RMS fix feed -->



<xs:simpleType name="AltTradeInfo">


<xs:annotation>

<xs:documentation>additional trade identifier information. Added to hold the RMS TRADEID field (FIX tag 90099) which, despite its name, is not the primary identifier of the trade in RMS</xs:documentation>

</xs:annotation>


<xs:restriction base="xs:string">

<xs:maxLength value="64"/>

</xs:restriction>

</xs:simpleType>


